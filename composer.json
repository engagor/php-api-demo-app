{
    "name": "engagor/engage-api-demo",
    "type": "library",
    "description": "",
    "keywords": [
        "engagor",
        "engage-api-demo"
    ],
    "homepage": "https://github.com/engagor/engage-api-demo",
    "license": "MIT",
    "authors": [
        {
            "name": "Engage Team",
            "email": "dev@engagor.com",
            "homepage": "https://github.com/engagor",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.1",
        "nyholm/psr7": "^1.2.0",
        "nyholm/psr7-server": "^0.4.1",
        "php-di/slim-bridge": "^3.0",
        "slim/slim": "^4.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=7.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Demo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Demo\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
